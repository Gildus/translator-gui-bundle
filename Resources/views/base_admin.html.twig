<!DOCTYPE html>
<html>
<head>
    <title>TranslatorGUIBundle</title>
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
   <!-- Bootstrap -->
    <link href="//maxcdn.bootstrapcdn.com/bootstrap/3.3.1/css/bootstrap.min.css" rel="stylesheet" media="screen">
    <link href="{{ asset('bundles/geschkeadmintranslatorgui/css/custom-admin.css') }}" rel="stylesheet" media="screen">

    {% block css %}{% endblock %}
    <script src="//ajax.googleapis.com/ajax/libs/jquery/1.11.1/jquery.min.js"></script>
    <!-- HTML5 shim and Respond.js IE8 support of HTML5 elements and media queries -->
    <!--[if lt IE 9]>
    <script src="//cdnjs.cloudflare.com/ajax/libs/html5shiv/3.7.2/html5shiv.min.js"></script>
    <script src="//cdnjs.cloudflare.com/ajax/libs/respond.js/1.4.2/respond.js"></script>
    <![endif]-->

</head>
<body>

<div class="navbar navbar-inverse navbar-fixed-top">
    <div class="container">
        <div class="navbar-header">
            <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
            </button>
            <a class="navbar-brand" href="{{ path('geschke_admin_translator_gui_homepage') }}">Translator GUI Bundle</a>
        </div>
        <div class="collapse navbar-collapse">
            <ul class="nav navbar-nav">
                <li {%  if mainnav == 'index' %}class="active"{% endif %}><a href="{{ path('geschke_admin_translator_gui_homepage') }}">{% trans %}nav.home{% endtrans %}</a></li>
                <li {%  if mainnav == 'bundles' %}class="active"{% endif %}><a href="{{ path('geschke_admin_translator_gui_bundles') }}">{% trans %}nav.bundles{% endtrans %}</a></li>
            </ul>
        </div><!--/.nav-collapse -->
    </div>
</div>




<div class="container-fluid">

    <div class="row">


        <div class="col-sm-12 col-md-12 main">
            {% block admincontent %}
            {% endblock %}
        </div>
    </div>



</div> <!-- /container -->



<script src="//maxcdn.bootstrapcdn.com/bootstrap/3.3.1/js/bootstrap.min.js"></script>



</body>
</html>




